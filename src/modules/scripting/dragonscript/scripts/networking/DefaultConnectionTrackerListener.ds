namespace Dragengine.Networking

pin Dragengine.Utils


/**
 * \brief Default implementation of ConnectionTrackerListener.
 */
class DefaultConnectionTrackerListener implements ConnectionTrackerListener
	/**
	 * \brief Create default listener.
	 */
	public func new()
	end
	
	
	
	/**
	 * \brief Connection has been added.
	 */
	public func void connectionAdded( ConnectionTracker tracker, Connection connection )
	end
	
	/**
	 * \brief Connection has been removed.
	 */
	public func void connectionRemoved( ConnectionTracker tracker, Connection connection )
	end
end
